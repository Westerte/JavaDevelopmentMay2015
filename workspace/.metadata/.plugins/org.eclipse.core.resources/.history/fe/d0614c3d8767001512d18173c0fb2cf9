pattern.login = ^[a-zA-Z][a-zA-Z0-9-_\\.]{1,20}$
pattern.password = ^((?=.*\\d))(?=.*[A-Z])(?=.*[a-z])\\w{8,30}$
pattern.email = ^[-\\w.]+@([A-z0-9][-A-z0-9]+\\.)+[A-z]{2,4}$
pattern.onlylitter = ^[\\w\\s]*$
pattern.date =  (?:0[1-9]|1\\d|2(?:\\d(?=\\.\\d\\d\\.(?:(?:[02468][048]|[13579][26])00|\\d\\d(?:[2468][48]|[13579][26]|0[48])))|[0-8])(?=\\.02)|2(?:\\d|[0-8])(?!\\.02)|3(?:0|[01](?=\\.(?:0[13578]|1[02])))(?!\\.02))\\.(?:0[1-9]|1[0-2])\\.\\d\{4\}
				
pattern.passportNumber = \\d\{6\}(?:(?:(?<=[0]\{6\})[1-9])|(?:(?<![0]\{6\})\\d))