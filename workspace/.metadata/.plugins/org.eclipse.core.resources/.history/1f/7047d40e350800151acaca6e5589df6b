elem1.type=composite

elem2.name=code
elem2.father=root
elem2.pattern=((\n|^).*?\\{([^}]|(?<!\n)\\})*\\}\\s*?\n)|(\\s*[A-Z_a-z]+\\s*((\\+|\\-|\\*|\\>\\>|\\<\\<|\\>\\>\\>|\\&|\\^|\\/)*\=)\\s*[A-Z_a-z0-9]+\\s*)
elem2.type=leaf

elem3.name=letter
elem3.father=code
elem3.pattern=([A-Z][^\\.\\?\\!\\:]*\\:\\s*((.*?\\{([^}]|(?<!\n)\\})*\\}\\s*?\n)|(\\s*[A-Z_a-z]+\\s*((\\+|\\-|\\*|\\>\\>|\\<\\<|\\>\\>\\>|\\&|\\^|\\/)*\=)\\s*[A-Z_a-z0-9]+\\s*)))|((\n|^).*?\\{([^}]|(?<!\n)\\})*\\}\\s*?\n)|([0-9]+\\.[0-9\\.\\s]*[A-Z_a-z_\\-\\s]+\n)|([A-Z][^\\.\\?\\!]*[\\.\\?\\!]+)
elem3.type=leaf

