elem1.name=code
elem1.father=root
elem1.pattern= (.*?\\{([^}]|(?<!\\n)\})*\\}\\s*?\\n)|((?<=[\\n|^])[\\w\\s]+([-+=/][\\w\\s]+?)+[;]*[\\s]*?\\n)
elem1.type=leaf

elem2.name=handle
elem2.father=root
elem2.pattern=((\\d\\.)+[^\\n]*\\n)(?=[A-Z]|(\\d\\.))
elem2.type=leaf

elem3.name=paragraph
elem3.father=root
elem3.pattern=
elem3.type=composite

elem4.name=sentence
elem4.father=paragraph
elem4.pattern=([\\s\\S]*?(([.?!]+?)|([:]\\s*?\\n))[\\s]*)
elem4.type=leaf
