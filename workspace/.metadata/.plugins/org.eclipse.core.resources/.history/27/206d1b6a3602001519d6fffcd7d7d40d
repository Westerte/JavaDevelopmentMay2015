package edu.nesterenko.airline.entity;

public enum Model {
	_747(Manufacturer.BOEING), 
	_777(Manufacturer.BOEING),
	
	A320(Manufacturer.AIRBUS),
	A380(Manufacturer.AIRBUS);
	
	
	public Manufacturer manufacturer;
	Model(Manufacturer manufacturer) {
		this.manufacturer = manufacturer; 
	}
	
	enum Manufacturer {
		BOEING,
		AIRBUS;
		
		@Override
		public String toString() {
			return manufacturer. + 		
		}
	}
	
	@Override
	public String toString() {
		return manufacturer. + 		
	}
}
