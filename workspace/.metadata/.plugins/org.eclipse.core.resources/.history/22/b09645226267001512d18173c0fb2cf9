pattern.login = ^[a-zA-Z][a-zA-Z0-9-_\\.]{1,20}$
pattern.password = ^((?=.*\\d))(?=.*[A-Z])(?=.*[a-z])\\w{8,30}$
pattern.email = ^[-\\w.]+@([A-z0-9][-A-z0-9]+\\.)+[A-z]{2,4}$
pattern.onlylitter = ^[\\w\\s]*$
pattern.date = (?:(?:[0-3](?:(?:(?<=[0])[1-9])|(?:(?<=[3])[0-1])|(?:(?<=[1-2])[0-9])))(?=(?:\\.[0][13578])|(?:\\.[1][02]))|(?:[0-3](?:(?:(?<=[0])[1-9])|(?:(?<=[3])[0])|(?:(?<=[1-2])[0-9])))(?=(?:\\.[0][469])|(?:\\.[1][1]))|(?:[0-2](?:(?:(?<=[0-1])[0-9])|(?:(?<=[2])[0-8])|(?:(?<=[2])[0-9](?=(?:(?:\\.\\d\\d\\.(?:(?:\\d\\d(?:(?:[2468][048])|(?:[13579][26])|(?:[0][48])))|(?:(?:[02468][048][0][0])|(?:[13579][26][0][0])))))))))(?=\\.[0][2]))\\.(?:[0-1](?:(?:(?<=[0])[1-9])|(?:(?<=[1])[0-2])))\\.\\d{4}