package edu.nesterenko.airline.entity;

public enum Model {
	_747(Manufacturer.BOEING, value), 
	_777(Manufacturer.BOEING),
	
	A320(Manufacturer.AIRBUS),
	A380(Manufacturer.AIRBUS);
	
	
	private Manufacturer manufacturer;
	private String value;
	
	Model(Manufacturer manufacturer, String value) {
		this.manufacturer = manufacturer; 
	}
	
	enum Manufacturer {
		BOEING,
		AIRBUS;
	}
}
