elem2.name=code
elem2.father=root
elem2.pattern= (.*?\\{([^}]|(?<!\\n)\})*\\}\\s*?\\n)|((?<=[\\n|^])[\\w\\s]+([-+=/][\\w\\s]+?)+[;]*[\\s]*?\\n)
elem2.type=leaf

elem1.name=handle
elem1.father=root
elem1.pattern=((\\d\\.)+[^\\n]*\\n)(?=[A-Z]|(\\d\\.))
elem1.type=leaf

elem3.name=paragraph
elem3.father=root
elem3.pattern=
elem3.type=composite

elem4.name=sentence
elem4.father=paragraph
elem4.pattern=([\\s\\S]*?(([.?!]+?)|([:]\\s*?\\n))[\\s]*)
elem4.type=composite

elem5.name=word
elem5.father=sentence
elem5.pattern=([\\w-]+)
elem5.type=leaf

elem6.name=punctuation
elem6.father=sentence
elem6.pattern=
elem6.type=leaf

elem7.name=separator
elem7.father=sentence
elem7.pattern=[\\s]+
elem7.type=leaf